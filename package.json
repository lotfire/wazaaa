    "csurf": "^1.9.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.8.1",
