{
  "name": "wazaaa",
  "version": "1.0.0",
  "description": "Demo app used in Delicious Insightsâ€™ Node.js training course",
  "private": true,
  "engines": {
    "node": ">= 6",
    "npm": ">= 4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev:debug": "nodemon --inspect src/boot.js",
    "dev:server": "nodemon src/boot.js",
    "dev:client": "webpack --progress --watch",
    "dev:test": "npm test -- --watch",
    "doc": "rimraf doc && groc",
    "lint": "standard --verbose | snazzy",
    "postinstall": "npm run build",
    "start": "node src/boot.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "npm test -- --coverage"
  },
  "babel": {
    "presets": [
      "latest-minimal",
      "stage-3"
    ]
  },
  "standard": {
    "env": {
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "keywords": [
    "node",
    "express",
    "connect",
    "passport",
    "social",
    "demo",
    "training",
    "jade",
    "webpack",
    "mocha",
    "tests"
  ],
  "author": "Christophe Porteneuve <christophe@delicious-insights.com> (http://delicious-insights.com)",
  "license": "No-License (see LICENSE file)",
